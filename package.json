{
  "name": "psych-city",
  "description": "a website",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && concurrently --kill-others \"budo --dir public/ --open --live\" \"npm run sass-watch\"",
    "copy": "cp src/admin/* public/admin",
    "dev": "netlify dev",
    "build": "mkdir -p public/admin && npm run copy && npm run sass",
    "sass": "sass src/style.scss public/style.css",
    "sass-watch": "sass --watch src/style.scss public/style.css",
    "test": "tape test/**/*.js"
  },
  "devDependencies": {
    "budo": "^11.6.4",
    "concurrently": "^5.3.0",
    "netlify-cli": "^2.69.11",
    "sass": "^1.32.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/psych-city.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nichoth/psych-city/issues"
  },
  "homepage": "https://github.com/nichoth/psych-city#readme"
}
